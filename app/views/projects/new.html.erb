<h1>Start a new Project</h1>
  <%= simple_form_for([@project]) do |f| %>
    <div class="form-inputs type-form-control">
      <%= f.input :title %>
      <%= f.rich_text_area :rich_description %>
      <%= f.input :start_date, label: "Start Date", as: :string, type: :text %>
      <%= f.input :end_date, label: "End Date", as: :string, type: :text %>
      <%= f.input :photo, as: :file %>
    </div>
    <div class="form-actions">
      <%= f.submit "Submit", class: "btn btn-flat" %>
    </div>
  <% end %>
